{
    "version": 2,
    "builds": [
      {
        "src": "personalPortfolio/**",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "staticfiles/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/(.*)",
        "dest": "personalPortfolio/wsgi.py"
      }
    ]
  }
  